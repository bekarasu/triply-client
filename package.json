{
  "name": "triply",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "dotenv -e .env.development -- expo start --port 8082",
    "start:dev": "dotenv -e .env.development -- expo start --port 8082",
    "start:staging": "dotenv -e .env.staging -- expo start --port 8082",
    "start:prod": "dotenv -e .env.production -- expo start --port 8082",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "dotenv -e .env.development -- expo start --android --port 8082",
    "android:dev": "dotenv -e .env.development -- expo start --android --port 8082",
    "android:staging": "dotenv -e .env.staging -- expo start --android --port 8082",
    "android:prod": "dotenv -e .env.production -- expo start --android --port 8082",
    "ios": "dotenv -e .env.development -- expo start --ios --port 8082",
    "ios:dev": "dotenv -e .env.development -- expo start --ios --port 8082",
    "ios:staging": "dotenv -e .env.staging -- expo start --ios --port 8082",
    "ios:prod": "dotenv -e .env.production -- expo start --ios --port 8082",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:android": "node ./scripts/build.js --env development --platform android --local",
    "build:android:dev": "npx eas build --profile development --platform android --local",
    "build:android:bundle": "expo build:android -t app-bundle",
    "build:ios": "expo build:ios",
    "build": "node ./scripts/build.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~53.0.25",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.8",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.10",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
